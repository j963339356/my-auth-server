<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.auth.demo.dao.UserInfoValidDao" >
    <resultMap id="UserInfoValidMap" type="com.auth.demo.mbg.model.Userinfo" extends="com.auth.demo.mbg.mapper.UserinfoMapper.BaseResultMap">

    </resultMap>
    <select id="validUserUsername" parameterType="java.lang.String" resultType="integer">
        select count(1) from userinfo where username = #{username,jdbcType=VARCHAR}
    </select>
</mapper>